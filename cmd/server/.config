# default environment, override with -env command line flag
env = dev

# development environment
dev.db.file	= users.db
dev.net.host	= 127.0.0.1
dev.net.port	= 8080

# production environment
prod.db.file	= users.db
prod.net.host	= 0.0.0.0
prod.net.port	= 8080

# timeout in minutes
dev.password.reset.timeout  = 5
prod.password.reset.timeout = 15

